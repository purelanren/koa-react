{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "global-require": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "semi": 0,
    "strict": 0,
    "arrow-parens": ["error", "as-needed"],
    "linebreak-style": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "import/prefer-default-export": 0
  }
}